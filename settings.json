    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "after": [],
            "before": [
                "u"
            ],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "<C-r>"
            ],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                ":w"
            ]
        },
        {
            "before": [
                "leader",
                "W"
            ],
            "commands": [
                "saveAll"
            ]
        },
        {
